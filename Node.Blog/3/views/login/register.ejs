<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head.ejs') %>
<body>
<%- include('../partials/nav.ejs') %>
<h1>Register</h1>

<div class="outerContainer">

    <form action="/login/login/register" method="POST">
        <input type="text" id="name"  placeholder="Name..." required name="name">
        <input type="text" id="username"  placeholder="Username..." required name="username">
        <input type="new-password" id="password" placeholder="Password..." required name="password">
        <input type="date" id="date" required name="date">
        <input type="text" id="email" placeholder="Email..." required name="email">
        <input type="text" id="occupation" placeholder="Your occupation..." required name="occupation">
        <button>Register new user</button>
    </form>
</div>
<%- include('../partials/footer.ejs') %>
</body>
</html>